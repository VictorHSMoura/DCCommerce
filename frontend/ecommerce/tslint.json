{
  "extends": "./node_modules/tslint-config-airbnb/tslint",
  "linterOptions": {
    "exclude": [
        "**/*.spec.ts",
        "src/test.ts",
        "src/polyfills.ts",
        "src/app/utils/functions.ts"
    ]
  },
  "rules": {
    "max-line-length": [
      true,
      180
    ],
    "no-parameter-reassignment": false
  }
}
